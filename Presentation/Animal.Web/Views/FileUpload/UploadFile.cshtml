@model Entities.FileUpload
@using Animal.Web.MediaComponents


@using (Html.BeginForm("UploadFile", "FileUpload", FormMethod.Post, new { enctype = "multipart/form-data", onSubmit = $"formSubmit('{Url.Action("UploadFile", "FileUpload")}', this, event);" }))
{
	@Html.AntiForgeryToken()
	<div>@Html.ValidationMessage("FormValidation")</div>

	@Html.FileInput(m=>m.files)

	@Html.SubmitButton("Submit form")
}
