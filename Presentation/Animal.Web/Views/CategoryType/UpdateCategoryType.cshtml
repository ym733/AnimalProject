@model ViewModel.CategoryType
@using Animal.Web.MediaComponents


@using (Html.BeginForm("UpdateCategoryType", "CategoryType", FormMethod.Post, new { enctype = "multipart/form-data", onSubmit = $"formSubmit('{Url.Action("UpdateCategoryType", "CategoryType")}', this, event);" }))
{
	@Html.AntiForgeryToken()
	<div>@Html.ValidationMessage("FormValidation")</div>

	@Html.TextInput(m=>m.Id)

	@Html.TextInput(m=>m.CategoryName)

	@Html.FileInput(m=>m.files)

	@Html.SubmitButton("Submit form")
}
