@model Entities.CategoryType

<table class="table table-bordered table-striped">
	<thead>
	<th class="p-3">ID</th>
	<th class="p-3">Name</th>
	<th class="p-3">Image</th>
	</thead>
	<tr>
		<td class="p-3">@Model.Id</td>
		<td class="p-3">@Model.CategoryName</td>
		<td class="p-3"><img style="width:30px;height:30px;" src="@Model.ImagePath" /></td>
	</tr>
</table>

