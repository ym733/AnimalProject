@model Entities.User
@{
	ViewData["title"] = "get user";
}

<div>
	<table class="table table-bordered table-striped">
		<thead>
			<th class="p-3">ID</th>
			<th class="p-3">Name</th>
			<th class="p-3">Email</th>
			<th class="p-3">Date of Birth</th>
			<th class="p-3">role</th>
		</thead>
		<tr>
			<td class="p-3">@Model.Id</td>
			<td class="p-3">@Model.Name</td>
			<td class="p-3">@Model.Email</td>
			<td class="p-3">@Model.DateOfBirth</td>
			<td class="p-3">@Model.role</td>

		</tr>
	</table>
</div>